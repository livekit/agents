# Filler-Aware Agent Configuration
# Copy this file to .env and fill in your values

# === LiveKit Connection ===
LIVEKIT_URL=wss://your-livekit-server.com
LIVEKIT_API_KEY=your_api_key
LIVEKIT_API_SECRET=your_api_secret

# === AI Provider Configuration ===
# OpenAI API (for LLM)
OPENAI_API_KEY=your_openai_api_key

# AssemblyAI API (for STT) - Optional if using other providers
ASSEMBLYAI_API_KEY=your_assemblyai_api_key

# Deepgram API (alternative STT) - Optional
DEEPGRAM_API_KEY=your_deepgram_api_key

# Cartesia API (for TTS) - Optional if using other providers
CARTESIA_API_KEY=your_cartesia_api_key

# === Model Selection ===
# Speech-to-Text provider
# Options: 'assemblyai/universal-streaming:en', 'deepgram', etc.
STT_PROVIDER=assemblyai/universal-streaming:en

# Large Language Model
# Options: 'openai/gpt-4o-mini', 'openai/gpt-4o', etc.
LLM_MODEL=openai/gpt-4o-mini

# Text-to-Speech provider  
# Options: 'cartesia/sonic-2', 'elevenlabs', 'openai', etc.
TTS_PROVIDER=cartesia/sonic-2

# === Filler Interruption Handler Configuration ===
# Comma-separated list of filler words to ignore during agent speech
# These will NOT interrupt the agent when it's speaking
# But WILL be processed as valid speech when agent is quiet
IGNORED_FILLER_WORDS=uh,um,umm,hmm,haan,yeah,huh,mhm,mm,er,ah

# === Interruption Behavior ===
# Time (in seconds) to wait before resuming after a potential false interruption
FALSE_INTERRUPTION_TIMEOUT=1.0

# === Logging ===
# Set log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO
